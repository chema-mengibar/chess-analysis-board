{
    "name": "chess-analysis-board",
    "version": "2.0.0",
    "description": "A chessboard for the browser in vanilla",
    "main": "index.js",
    "scripts": {
        "start": "cross-env NODE_PATH=. NODE_ENV=development webpack serve --open --config webpack.dev.js",
        "build": "cross-env NODE_PATH=. NODE_ENV=production webpack --config webpack.prod.js",
        "test": "jest --config jest.config.js",
        "simple-server": "node server.js"
    },
    "keywords": [
        "chess",
        "vanilla",
        "analysis",
        "chessboard"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/chema-mengibar/chess-analysis-board"
    },
    "bugs": {
        "url": "https://github.com/chema-mengibar/chess-analysis-board/issues"
    },
    "homepage": "http://motuo.info/blog/dist/#/project/chess-analysis-board",
    "author": "chema-mengibar",
    "license": "MIT",
    "devDependencies": {
        "cross-env": "^7.0.3",
        "css-loader": "^5.1.3",
        "html-webpack-plugin": "^5.3.1",
        "jest": "^26.6.3",
        "jest-esm-transformer": "^1.0.0",
        "mini-css-extract-plugin": "^1.3.9",
        "node-sass": "^5.0.0",
        "sass-loader": "^11.0.1",
        "style-loader": "^2.0.0",
        "webpack": "^5.27.1",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.3"
